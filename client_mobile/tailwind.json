{
	"mt-2": {
		"style": {
			"marginTop": 8
		}
	},
	"flex": {
		"style": {
			"display": "flex"
		}
	},
	"flex-1": {
		"style": {
			"flexGrow": 1,
			"flexShrink": 1,
			"flexBasis": "0%"
		}
	},
	"items-center": {
		"style": {
			"alignItems": "center"
		}
	},
	"justify-center": {
		"style": {
			"justifyContent": "center"
		}
	},
	"rounded-md": {
		"style": {
			"borderTopLeftRadius": 6,
			"borderTopRightRadius": 6,
			"borderBottomRightRadius": 6,
			"borderBottomLeftRadius": 6
		}
	},
	"bg-black": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(0 0 0 / var(--tw-bg-opacity))"
		}
	},
	"bg-blue-600": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(37 99 235 / var(--tw-bg-opacity))"
		}
	},
	"p-4": {
		"style": {
			"paddingTop": 16,
			"paddingRight": 16,
			"paddingBottom": 16,
			"paddingLeft": 16
		}
	},
	"p-2": {
		"style": {
			"paddingTop": 8,
			"paddingRight": 8,
			"paddingBottom": 8,
			"paddingLeft": 8
		}
	},
	"text-center": {
		"style": {
			"textAlign": "center"
		}
	},
	"text-4xl": {
		"style": {
			"fontSize": 36,
			"lineHeight": 40
		}
	},
	"text-lg": {
		"style": {
			"fontSize": 18,
			"lineHeight": 28
		}
	},
	"text-blue-600": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(37 99 235 / var(--tw-text-opacity))"
		}
	},
	"text-gray-600": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(75 85 99 / var(--tw-text-opacity))"
		}
	},
	"text-white": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(255 255 255 / var(--tw-text-opacity))"
		}
